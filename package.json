  "homepage": "https://unorich.github.io/",
  "dependencies": {
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.6.1",
    "gh-pages": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.0.3",
    "react-scripts": "2.0.3",
    "redux": "^4.0.1"
